{
  "name": "Device",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strict":"filter"
  },
  "mixins": {
    "DisableCommon": true,
    "EnableHooks": true,
    "ChangeTracker": false,
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    },
    "Util": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "os": {
      "type": "string"
    },
    "userAgent": {
      "type": "string"
    },
    "locale": {
      "type": "string"
    },
    "deviceType": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "uploadPowerUsage": {
      "type": "boolean",
      "default": false
    },
    "uploadHighFrequencyPowerUsage": {
      "type": "boolean",
      "default": false
    },
    "tapToToggleCalibration": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "currentLocation": {
      "type": "belongsTo",
      "model": "Location",
      "foreignKey": "currentLocationId"
    },
    "currentSphere": {
      "type": "belongsTo",
      "model": "Sphere",
      "foreignKey": "currentSphereId"
    },
    "installations": {
      "type": "hasMany",
      "model": "AppInstallation",
      "foreignKey": "deviceId"
    },
    "fingerprintLinks": {
      "type": "hasMany",
      "model": "FingerprintLinker",
      "foreignKey": "deviceId"
    },
    "preferences": {
      "type": "hasMany",
      "model": "Preference",
      "foreignKey": "deviceId"
    },
    "trackingNumber": {
      "type": "hasMany",
      "model": "SphereTrackingNumber",
      "polymorphic": "trackingNumber"
    }
  },
  "acls": [],
  "methods": {}
}
