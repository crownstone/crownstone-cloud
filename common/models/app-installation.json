{
  "name": "AppInstallation",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "strict":"filter"
  },
  "mixins": {
    "DisableCommon": true,
    "ChangeTracker": false,
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "appName":{
      "type":"string",
      "required": true
    },
    "appVersion": {
      "type": "string"
    },
    "deviceType": {
      "type": "string",
      "required": true
    },
    "deviceToken": {
      "type": "string",
      "required": false
    },
    "installationToken": {
      "type": "string",
      "required": false
    },
    "developmentApp": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "hidden": [
    "installationToken"
  ],
  "relations": {
    "device": {
      "type": "belongsTo",
      "model": "Device",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
