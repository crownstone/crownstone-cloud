{
  "name": "Sphere",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strict":"filter"
  },
  "mixins": {
    "DisableCommon": true,
    "EnableHooks": true,
    "ChangeTracker": false,
    "Cascade": {
      "SphereAccess": "sphereId",
      "Location": "sphereId",
      "SphereKeys": "sphereId",
      "StoneKeys": "sphereId"
    },
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    },
    "Util": true
  },
  "hidden": [
    "adminEncryptionKey",
    "memberEncryptionKey",
    "guestEncryptionKey"
  ],
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "uid": {
      "type": "number"
    },
    "uuid": {
      "type": "string",
      "required": true
    },
    "adminEncryptionKey": {
      "type": "string"
    },
    "memberEncryptionKey": {
      "type": "string"
    },
    "guestEncryptionKey": {
      "type": "string"
    },
    "aiName": {
      "type": "string"
    },
    "aiSex": {
      "type": "string"
    },
    "exitDelay": {
      "type": "number",
      "default": 600
    },
    "gpsLocation": {
      "type": "GeoPoint"
    }
  },
  "validations": [],
  "relations": {
    "ownedLocations": {
      "type": "hasMany",
      "model": "Location",
      "foreignKey": "sphereId"
    },
    "Toons": {
      "type": "hasMany",
      "model": "Toon",
      "foreignKey": "sphereId"
    },
    "ownedStones": {
      "type": "hasMany",
      "model": "Stone",
      "foreignKey": "sphereId"
    },
    "events": {
      "type": "hasMany",
      "model": "Change",
      "foreignKey": "sphereId"
    },
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "sphereId",
      "through": "SphereAccess"
    },
    "messages": {
      "type": "hasMany",
      "model": "Message",
      "foreignKey": "sphereId"
    },
    "hubs": {
      "type": "hasMany",
      "model": "Hub",
      "foreignKey": "sphereId"
    },
    "scenes": {
      "type": "hasMany",
      "model": "Scene",
      "foreignKey": "sphereId"
    },
    "sortedLists": {
      "type": "hasMany",
      "model": "SortedList",
      "foreignKey": "sphereId"
    },
    "features": {
      "type": "hasMany",
      "model": "SphereFeature",
      "foreignKey": "sphereId"
    },
    "geoLocation": {
      "type":"hasOne",
      "model": "geoLocation",
      "foreignKey": "sphereId"
    },
    "floatingLocationPosition": {
      "type":"hasOne",
      "model": "Position",
      "foreignKey": "sphereId"
    }
  },
  "acls": [],
  "methods": {}
}
